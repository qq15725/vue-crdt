{
  "name": "vue-crdt",
  "type": "module",
  "version": "0.0.0",
  "packageManager": "pnpm@8.7.6",
  "description": "CRDT in Vue.js",
  "author": "wxm",
  "license": "MIT",
  "homepage": "https://github.com/qq15725/vue-crdt",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qq15725/vue-crdt.git"
  },
  "bugs": {
    "url": "https://github.com/qq15725/vue-crdt/issues"
  },
  "keywords": [
    "reactive",
    "crdt",
    "yjs",
    "sync",
    "automerge",
    "collaboration",
    "collaborative",
    "mobx",
    "vue",
    "vuejs",
    "observable",
    "operational transforms",
    "conflict",
    "offline",
    "shared editing"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "dist/index.mjs",
  "module": "dist/index.mjs",
  "types": "types/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./types/*",
        "./types/index.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "types"
  ],
  "scripts": {
    "dev": "pnpm -F docs dev",
    "lint": "eslint src",
    "test": "vitest --no-threads --no-isolate",
    "build": "vite build && tsc --project tsconfig.build.json",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
    "release": "bumpp package.json --commit \"release: v%s\" --push --all --tag"
  },
  "devDependencies": {
    "@qq15725/eslint-config": "^1.1.0",
    "@types/node": "^18.14.0",
    "bumpp": "^8.2.1",
    "conventional-changelog-cli": "^2.2.2",
    "eslint": "^8.34.0",
    "typescript": "^4.9.5",
    "vite": "^4.1.2",
    "vitest": "^0.28.5",
    "vue": "^3.3.13",
    "y-webrtc": "^10.3.0",
    "yjs": "^13.6.10"
  },
  "peerDependencies": {
    "vue": "^3.*",
    "yjs": "^13.*"
  }
}
